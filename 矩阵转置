#include <stdio.h>
#include <stdlib.h>
#define N 10
int main()
{
    int  a[N][N],i,j,x,y,b,c,n;
    scanf("%d",&n);
    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
    {
        a[i][j]=i*n+j+1;
    }
    printf("The original matrix is:\n");
    for(x=0;x<n;x++)
    {
        for(y=0;y<n;y++)
        {
            printf("%3d",a[x][y]);
        }
        printf("\n");
    }
    printf("The changed matrix is:\n");
    for(c=0;c<n;c++)
    {
        for(b=0;b<n;b++)
        {
           printf("%3d",a[b][c]);
        }
        printf("\n");
    }



}
